{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint ./",
    "eslint:prettier": "eslint-config-prettier ./src/index.js",
    "prettier:check": "prettier --check ./",
    "prettier:write": "prettier --write ./",
    "dev": "nodemon --exec babel-node ./src/index.js",
    "dev:docker": "nodemon --legacy-watch --exec babel-node ./src/index.js",
    "build": "babel ./src --out-dir ./build",
    "start": "node ./build/index.js",
    "queues:dev": "nodemon --exec babel-node ./src/queues/index.js",
    "queues:docker": "nodemon --legacy-watch --exec babel-node ./src/queues/index.js",
    "queues": "node ./build/queues/index.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed:all": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo",
    "seed:undo:all": "sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "bull": "^4.10.4",
    "express": "^4.18.2",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/node": "^7.22.6",
    "@babel/preset-env": "^7.22.9",
    "@babel/register": "^7.22.5",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "sequelize-cli": "^6.6.1"
  }
}
